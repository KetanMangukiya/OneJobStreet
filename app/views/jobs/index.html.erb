<table class="table table-striped">
  <tr>
    <th>Company</th>
    <th>Job Type</th>
    <th>Role</th>
    <th>Vacancy</th>
    <th>Description</th>
    <th>Salary</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Status</th>
    <th>Action</th>

  </tr>
  <%@jobs.each do |job|%>
  <tr>
    <td><%=job.company.name%></td>
    <td><%=job.job_type.name%></td>
    <td><%=job.role.name%></td>
    <td><%=job.vacancy%></td>
    <td><%=job.description%></td>
    <td><%=job.salary%></td>
    <td><%=job.start_date%></td>
    <td><%=job.end_date%></td>
    <td><%=job.status%></td>
    <td>
      <%= link_to "Show",job %>
      <%= link_to "Edit", edit_job_path(job) %>
  		<%= link_to "Delete",job,method: :delete, data:{confirm: "Are you sure to delete?"} %></center></td>
    </td>
  </tr>
  <%end%>
</table>
<%=link_to "Add Job Post" , new_job_path %>
